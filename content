    $(document).ready(function() {
      $('#scroll').hide();
      $

      function() {
        $(window).scroll(function() {
            if ($(this).scrollTop() > 200) {
              $('#scroll').fadeIn();
            } else
          }
          $('#scroll').fadeOut();
        }
      });
    });
    $('#scroll a').click(function() {
    $('body,html').animate({
      scrollTop: 0
    }, 300);
    return false;
    });
    });
  </script>
